<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
		"-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>

	<classPathEntry location="E:\maven_repository\mysql\mysql-connector-java\5.1.8\mysql-connector-java-5.1.8.jar"/>

	<context id="context" targetRuntime="MyBatis3">
		<commentGenerator>
			<!--不生成注释-->
			<property name="suppressAllComments" value="true"/>
			<property name="suppressDate" value="true"/>
		</commentGenerator>

		<jdbcConnection userId="root" password="root" driverClass="com.mysql.jdbc.Driver"
		                connectionURL="jdbc:mysql://127.0.0.1:3306/storage?characterEncoding=UTF-8&amp;serverTimezone=CTT"/>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false"/>
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.creating.dao.mapper.entity" targetProject=".">
			<property name="enableSubPackages" value="false"/>
			<property name="trimStrings" value="true"/>
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.creating.dao.mapper.goods" targetProject=".">
			<property name="enableSubPackages" value="false"/>
		</sqlMapGenerator>

		<javaClientGenerator targetPackage="com.creating.dao.mapper.goods" type="XMLMAPPER" targetProject=".">
			<property name="enableSubPackages" value="false"/>
		</javaClientGenerator>


		<!--<table schema="storage" tableName="t_goods" domainObjectName="goods.GoodsEty"
		       enableInsert="true"
		       enableSelectByPrimaryKey="true"
		       enableUpdateByPrimaryKey="true"
		       enableDeleteByPrimaryKey="true"
		       enableCountByExample="false"
		       enableDeleteByExample="false"
		       enableSelectByExample="false"
		       enableUpdateByExample="false"/>-->


		<table schema="storage" tableName="t_recieve_info" domainObjectName="goods.RecieveInfo"
		       enableInsert="true"
		       enableSelectByPrimaryKey="true"
		       enableUpdateByPrimaryKey="true"
		       enableDeleteByPrimaryKey="true"
		       enableCountByExample="false"
		       enableDeleteByExample="false"
		       enableSelectByExample="false"
		       enableUpdateByExample="false"/>


		<table schema="storage" tableName="t_recieve_items" domainObjectName="goods.RecieveItems"
		       enableInsert="true"
		       enableSelectByPrimaryKey="true"
		       enableUpdateByPrimaryKey="true"
		       enableDeleteByPrimaryKey="true"
		       enableCountByExample="false"
		       enableDeleteByExample="false"
		       enableSelectByExample="false"
		       enableUpdateByExample="false"/>
	</context>
</generatorConfiguration>